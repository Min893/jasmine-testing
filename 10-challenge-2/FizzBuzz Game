describle ("fizzBuzz", function(){
beforeEach function(){
fizzBuzz=new fizzBuzz();
})

describe("division method", function(number){
it("should return a new fizzBuzz if the number is divisible by 3 and 5 or either", funcion(){
expect(fizzBuzz), toBeDefined;
})

it("should return 'FizzBuzz' if the number is divisible by 3 and 5", function(){
expect(fizzBuzz), toBe(FizzBuzz);
})

it("should return 'Fizz' if the number is divisible by 3", function(){
expect(fizzBuzz), toBe(Fizz);
})

it("should return 'Buzz' if the number is divisible by 5", function(){
expect(fizzBuzz), toBe(Buzz);
})

else { return (the number, function(){
expect(fizzBuzz), toBe(number);
})
